# Supabase Configuration (Required)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Supabase Storage Bucket (Optional, defaults to "raw-ingest")
SUPABASE_STORAGE_BUCKET=raw-ingest

# n8n Webhook URLs
N8N_WEBHOOK_URL=https://optio-xrl.app.n8n.cloud/webhook/xrl/session/create
N8N_UPLOAD_WEBHOOK_URL=https://optio-xrl.app.n8n.cloud/webhook/webhook/xrl/document/upload

# n8n Callback Secret (Optional)
# If set, this header will be forwarded to n8n webhooks
X_CALLBACK_SECRET=

# Embeddings (Optional, defaults to false)
# Set to "true" to enable embeddings (not implemented in MVP)
ENABLE_EMBEDDINGS=false

# Server Configuration
PORT=3001

# CORS Configuration
FRONTEND_ORIGIN=http://localhost:5173
